 <header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">
    <%= link_to "Office of Student Housing", root_path, id: "logo" %>
    <nav>
      <% if current_user %>
      <ul class="nav navbar-nav pull-right">
        <li><%= link_to "Home", root_path %></li>
        <li><%= link_to "Apply", new_application_path %></li>
        <li><%= link_to "Dorms", view_dorms_path %></li>
        <% if Admin.authenticate(current_user.username) != nil %>
          <li><%= link_to "Post", admin_post_path %></li>
          <li><%= link_to "Waitlist", admin_waitlist_path %></li>
        <% end %>
        <li><%= link_to "Logout", logout_path, method: :post%></li>
      </ul>
      <% end %>
    </nav>

  </div>
</header>